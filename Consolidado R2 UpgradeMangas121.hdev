<l>            endif</l>
<l>            clear_object_model_3d([ObjectModel3D_Core_Pallet])</l>
<l>*             clear_surface_model (SFM_Core_Pallet)</l>
<l>        endif</l>
<c>        </c>
<l>    endfor</l>
<l>catch (Exception)</l>
<l>    sdf:=123  </l>
<c>    </c>
<l>endtry</l>
<l>Vector_Existencia_Core_New:=Vector_Existencia_Core</l>
