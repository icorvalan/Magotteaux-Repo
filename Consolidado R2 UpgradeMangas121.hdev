<procedure name="Get_mangaPituto">
<interface>
<ic>
<par name="NxLib" base_type="ctrl" dimension="0"/>
<par name="Camera_3" base_type="ctrl" dimension="0"/>
<par name="DiametroSleeve" base_type="ctrl" dimension="0"/>
