<l>tuple_split (Variable, ';', Data)</l>
<c></c>
<l>Pattern:=Data[0]</l>
<l>PosicionRack:=Data[1]</l>
<l>if (Data[2]='O')</l>
<l>    ExisteEnRack:=true</l>
<l>elseif (Data[2]='X')</l>
<l>    ExisteEnRack:=false</l>
<l>endif</l>
<l>return ()</l>
</body>
