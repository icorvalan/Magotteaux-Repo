<par name="FXcheck" base_type="ctrl" dimension="0"/>
<par name="Diametro" base_type="ctrl" dimension="0"/>
</oc>
</interface>
<body>
<c></c>
<l>FXcheck := false</l>
<l>for Index := 1 to 5 by 1</l>
<c></c>
<c></c>
<l>    try</l>
<l>        Variable_BD := 'ram_actualproduction'</l>
