<l>        Estado_Funcion := true</l>
<l>        return()</l>
<c>        </c>
<l>    endif</l>
<c>    </c>
<l>    Estado_Funcion := false</l>
<l>catch (Exception)</l>
<l>    Estado_Funcion := false</l>
<c>    * * --&gt; descomentar EN c#  Fx_Aux.DataLogError = Fx_Aux.ErrorCatch(HDevExpDefaultException1);</c>
<c>    * </c>
<l>endtry</l>
<c>* </c>
<c>* </c>
<l>return ()</l>
