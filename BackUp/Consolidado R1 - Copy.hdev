<parameter id="Escena3D_RAW"/>
<parameter id="Parametros_Visualizacion"/>
<parameter id="Path_3D_Segmentos"/>
<parameter id="Path_CORE_ID"/>
<parameter id="Path_SFM_Segmentos"/>
<parameter id="ROI3D_rotate"/>
